{
    "name": "rmlev/auth0-login-bundle",
    "description": "A Symfony bundle for authentication with the Auth0 service",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "oauth",
        "auth0",
        "security"
    ],
    "autoload": {
        "psr-4": {
            "Rmlev\\Auth0LoginBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rmlev\\Auth0LoginBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Roman",
            "email": "rlev0109@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0, <8.2",
        "symfony/http-client": "^4.4 || ^5.0 || ^6.0",
        "symfony/psr-http-message-bridge": "^v2.1",
        "nyholm/psr7": "^1.4",
        "auth0/auth0-php": "^8.0",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/security-bundle": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "dg/bypass-finals": "^v1.3",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0",
        "doctrine/orm": "^2.10",
        "doctrine/doctrine-bundle": "^2.5",
        "symfony/browser-kit": "^4.4 || ^5.0 || ^6.0",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/extension-installer": "^1.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "phpstan/extension-installer": true
        }
    }
}
